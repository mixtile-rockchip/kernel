/dts-v1/;
/plugin/;

#include <dt-bindings/interrupt-controller/irq.h>
#include <dt-bindings/pinctrl/rockchip.h>

/ {
	fragment@0 {
		target = <&uart1>;

		__overlay__ {
			status = "okay";
			pinctrl-0 = <&uart1m1_xfer &uart1m1_ctsn &uart1m1_rtsn>;
		};
	};

	fragment@1 {
                target = <&can1>;

                __overlay__ {
                        status = "okay";
                        pinctrl-names = "default";
                        pinctrl-0 = <&can1m1_pins>;
                };
        };

	fragment@2 {
		target = <&i2c5>;

		__overlay__ {
                        status = "okay";
                        pinctrl-names = "default";
                        pinctrl-0 = <&i2c5m2_xfer>;
		};
	};

	fragment@3 {
		target = <&i2s0_8ch>;
		
		__overlay__ {
			status = "okay";
			pinctrl-names = "default";
			pinctrl-0 = <&i2s0_lrck
			             &i2s0_sclk
			             &i2s0_sdi0
			             &i2s0_sdo0>;
		};
	};

	fragment@4 {
		target = <&spi0>;

		__overlay__ {
			status = "okay";
			pinctrl-names = "default";
			pinctrl-0 = <&spi0m3_cs1 &spi0m3_pins>;
			num-cs = <1>;
			
			spi_dev@0 {
			        compatible = "rockchip,spidev";
			        reg = <0>;
			        spi-max-frequency = <50000000>;
			};
		};	
	};	

	fragment@5 {
		target = <&spi1>;
		
		__overlay__ {
			status = "okay";
			pinctrl-names = "default";
			pinctrl-0 = <&spi1m2_cs0 &spi1m2_pins>;
			num-cs = <1>;
			
			spi_dev@0 {
			        compatible = "rockchip,spidev";
			        reg = <0>;
			        spi-max-frequency = <50000000>;
			};
		};
	};
};

